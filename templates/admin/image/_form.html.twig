<form action="{{ path('admin_image_new',{'aid': aid}) }}" method="post" enctype="multipart/form-data">
    Select image to upload:
    <input name="productid" type="hidden" value="{{ aid }}" type="number" readonly>
    <input type="file" name="imagename" id="fileToUpload"><br>
    <input type="submit" value="Upload Image" name="submit">
</form>